SERV="test.sorcery.net"
NET="SorceryNet"
ID=3
SEED="0x1234"

if [ "$1" = "-name" ] ; then
   echo $SERV
fi

if [ "$1" = "-id" ] ; then
 echo $ID
fi

if [ "$1" = "-net" ] ; then
 echo $NET
fi

if [ "$1" = "-blurb" ] ; then
cat << EOF
     Network                 :  $NET
     Server Name             :  $SERV
     Unique Server ID Number :  $ID
EOF
fi

if [ "$1" = "-make" ] ; then
 echo
fi

if [ "$1" = "-seed" ] ; then
  echo $SEED
fi

if [ "$1" = "-select" ] ; then
 echo
fi
