<help service="chanserv">
 <topic name="LISTAK">
  <command name="LISTAK">
   <syntax>
     /ChanServ LISTAK #&lt;channel&gt;
   </syntax>
 
   <desc>
     LISTAK allows you to list all akicks on a specified
     channel.
   </desc>

   <examples> 
     <ex pre="To list the akicks for #Voyager:">
       /ChanServ listak #Voyager
     </ex>
   </examples>
  </command>
 </topic>
</help>
